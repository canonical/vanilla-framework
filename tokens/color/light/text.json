{
  "$type": "color",
  "color": {
    "text": {
      "positive": {
        "themeable": true,
        "$value": "{color.base.positive}"
      },
      "negative": {
        "themeable": true,
        "$value": "{color.base.negative}"
      },
      "information": {
        "themeable": true,
        "$value": "{color.base.information}"
      },
      "caution": {
        "themeable": true,
        "$value": "{color.base.caution}"
      },
      "default": {
        "themeable": true,
        "$value": "#000000"
      },
      "muted": {
        "themeable": true,
        "$value": "rgba(0, 0, 0, 0.6)"
      },
      "inactive": {
        "themeable": true,
        "$value": "rgba(0, 0, 0, 0.75)"
      },
      "button": {
        "brand": {
          "themeable": true,
          "$value": "#ffffff"
        },
        "default": {
          "themeable": true,
          "$value": "{color.text.default}"
        },
        "base": {
          "themeable": true,
          "$value": "{color.text.default}"
        },
        "positive": {
          "themeable": true,
          "$value": "#ffffff"
        },
        "negative": {
          "themeable": true,
          "$value": "#ffffff"
        }
      },
      "link": {
        "default": {
          "themeable": true,
          "$value": "#0066CC"
        },
        "visited": {
          "themeable": true,
          "$value": "#7D42B8"
        }
      }
    }
  }
}
