{
  "$type": "color",
  "color": {
    "border": {
      "highlight": {
        "themeable": true,
        "$value": "#000000"
      },
      "accent": {
        "themeable": true,
        "$value": "#0F95A1"
      },
      "focus": {
        "themeable": true,
        "$value": "#2E96FF"
      },
      "default": {
        "themeable": true,
        "$value": "rgba(0, 0, 0, 0.2)"
      },
      "high-contrast": {
        "themeable": true,
        "$value": "rgba(0, 0, 0, 0.56)"
      },
      "low-contrast": {
        "themeable": true,
        "$value": "rgba(0, 0, 0, 0.1)"
      },
      "neutral": {
        "themeable": true,
        "$value": "rgba(0, 0, 0, 0.56)"
      },
      "positive": {
        "themeable": true,
        "$value": "{color.base.positive}"
      },
      "negative": {
        "themeable": true,
        "$value": "{color.base.negative}"
      },
      "information": {
        "themeable": true,
        "$value": "{color.base.information}"
      },
      "caution": {
        "themeable": true,
        "$value": "{color.base.caution}"
      }
    }
  }
}
